    "oat:sign": "oat sign OB.Obigo-SY.SsangyongLv2 ./build",
    "oat:pack": "oat pack -a podcast-dev ./build",
    "oat:push": "oat push -t root:root@10.28.3.190 podcast-dev.wgt /home/root/OAD/OAF/AFAsset/wi_apps",
