    "build:all": "node build.config/build.js && npm run build --prefix gadgets/main && npm run build --prefix gadgets/sub-1",
    "build:dev": "node build.config/build.dev.js",
    "build:dev-all": "node build.config/build.dev.js & cd gadgets/main/ & npm run build:dev & cd ../sub-1/ & npm run build:dev & cd ../sub-2/ & npm run build:dev",
